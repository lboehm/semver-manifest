{
  "name": "semver-manifest",
  "version": "0.1.5",
  "description": "Semantic versioning of manifest.json",
  "main": "./lib/index.js",
  "bin": {
    "semver-manifest": "./lib/index.js"
  },
  "scripts": {
    "start": "node src/index.js patch --profile ui5",
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "nyc --check-coverage npm run test:only",
    "lint": "eslint src test",
    "lint-fix": "eslint src test --fix",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepare": "npm run clean && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lboehm/semver-manifest.git"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "semantic",
    "versioning",
    "manifest.json"
  ],
  "author": "Lukas BÃ¶hm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lboehm/semver-manifest/issues"
  },
  "homepage": "https://github.com/lboehm/semver-manifest#readme",
  "dependencies": {
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2"
  },
  "nyc": {
    "branches": 70,
    "lines": 70,
    "functions": 70,
    "statements": 70
  }
}
